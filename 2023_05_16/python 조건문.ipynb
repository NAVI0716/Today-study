   ],
   "source": [
    "month = int(input(\"월을 입력하세요 :\"))\n",
    "if month >=3 and month <=5:\n",
    "    print(\"%s월은 봄입니다.\"%month)\n",
    "elif month >= 6 and month <=8:\n",
    "    print(\"%s월은 여름입니다.\"%month)\n",
    "elif month >= 9 and month <= 11:\n",
    "    print(\"%s월은 가을입니다.\"%month)\n",
    "else:\n",
    "    print(\"%s월은 겨울입니다.\"%month)"
   ]
